{
  "name": "dam-pharm",
  "version": "0.0.0",
  "license": "MIT",
  "description": "The Light/Responsive Inventory Management System.",
  "main": "app/main.js",
  "author": {
    "name": "Dam Pharm",
    "email": "support@salahatwa.web.id",
    "url": "https://javatars.web.id/"
  },
  "build": {
    "appId": "com.javatars.storaji",
    "productName": "DamPharm",
    "files": [
      "app/**"
    ],
    "directories": {
      "output": "dist"
    },
    "mac": {
      "category": "public.app-category.business"
    },
    "linux": {
      "category": "Office",
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "nsis": {
      "allowToChangeInstallationDirectory": true,
      "shortcutName": "DamPharm",
      "oneClick": false,
      "perMachine": true,
      "menuCategory": true,
      "artifactName": "DamPharmSetup.exe"
    }
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "ng build -prod",
    "postbuild": "npm run deploy",
    "predeploy": "rimraf ../resources/static/ && mkdirp ../resources/static && mkdirp ../resources/static/assets && mkdirp ../resources/static/assets/i18n",
    "deploy": "copyfiles -f dist/** ../resources/static && copyfiles -f src/assets/** ../resources/static/assets && copyfiles -f src/assets/i18n/** ../resources/static/assets/i18n",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.2.4",
    "@angular/common": "^4.2.4",
    "@angular/compiler": "^4.2.4",
    "@angular/core": "^4.2.4",
    "@angular/forms": "^4.2.4",
    "@angular/http": "^4.2.4",
    "@angular/platform-browser": "^4.2.4",
    "@angular/platform-browser-dynamic": "^4.2.4",
    "@angular/router": "^4.2.4",
    "@fengyuanchen/datepicker": "^0.6.3",
    "@ngui/auto-complete": "^0.16.0",
    "@ngx-translate/core": "^8.0.0",
    "@ngx-translate/http-loader": "^2.0.0",
    "@types/highcharts": "^5.0.16",
    "@types/lodash": "^4.14.77",
    "@types/tus-js-client": "^1.4.1",
    "angular2-highcharts": "^0.5.5",
    "copyfiles": "^1.2.0",
    "core-js": "^2.4.1",
    "electron-is-dev": "~0.3.0",
    "electron-log": "~2.2.14",
    "electron-updater": "~2.19.1",
    "font-awesome": "^4.7.0",
    "gasparesganga-jquery-loading-overlay": "^1.5.3",
    "highcharts": "^6.0.6",
    "ionicons": "^2.0.1",
    "jquery": "^3.2.1",
    "jquery.transit": "^0.9.12",
    "lodash": "^4.17.5",
    "mkdirp": "^0.5.1",
    "moment": "^2.20.1",
    "ngx-pagination": "^3.2.0",
    "ngx-progressbar": "^2.1.1",
    "notyf": "^2.0.1",
    "numeral": "^2.0.6",
    "offline-js": "^0.7.19",
    "pdfmake": "^0.1.33",
    "rimraf": "^2.6.1",
    "rxjs": "^5.4.2",
    "tus-js-client": "^1.4.5",
    "uikit": "^3.0.0-beta.30",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.4",
    "@angular/compiler-cli": "^4.2.4",
    "@angular/language-service": "^4.2.4",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "~3.1.1",
    "electron": "~1.7.10",
    "electron-builder": "~19.54.0",
    "icon-gen": "^1.2.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "npm-run-all": "^4.1.2",
    "protractor": "~5.1.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3",
    "webpack": "^3.11.0"
  }
}
